[flake8]
max-line-length = 100
exclude = 
    migrations,
    __pycache__,
    venv,
    env,
    .git,
    .tox,
    dist,
    build,
    *.egg
ignore = 
    E203,  # whitespace before ':'
    W503,  # line break before binary operator
    F401,  # imported but unused (in __init__.py files)
per-file-ignores =
    __init__.py:F401
max-complexity = 10